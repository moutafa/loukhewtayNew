<span style="font-size: 14px;">Je veux partager des événements</span><br/>
<br/>
<span style="font-size: 12px; font-family: verdana, tahoma;">Je m'inscrit</span><br/>
<br/>
<div class="login-fb"></div>
<br/>
<%= form_for(@user) do |f| %>
  <% if @user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% @user.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <table>
    <tr>
      <td><%= f.label :prenom %></td>
      <td><%= f.text_field :prenom %></td>
    </tr>
    <tr>
      <td><%= f.label :nom %></td>
      <td><%= f.text_field :nom %></td>
    </tr>
    <tr>
      <td><%= f.label :login %></td>
      <td><%= f.text_field :login %></td>
    </tr>
    <tr>
      <td><%= f.label :password %></td>
      <td><%= f.password_field :password %></td>
    </tr>
    <tr>
      <td><%= f.label :email %></td>
      <td><%= f.email_field :email %></td>
    </tr>
    <tr>
      <td><%= f.label :telephone %></td>
      <td><%= f.text_field :telephone %></td>
    </tr>
    <tr>
      <td><%= f.hidden_field :idfacebook %></td>
    </tr>
    <tr>
      <td><%= f.submit "Valider"%></td>
    </tr>
  </table>

<% end %>
